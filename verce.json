{
  "cleanUrls": true,
  "headers": [
    {
      "source": "/object-sans/(.*)",
      "headers": [
        { "key": "Access-Control-Allow-Origin", "value": "*" },
        { "key": "Content-Type", "value": "font/woff2" }
      ]
    }
  ],
  "rewrites": [
    {
      "source": "/object-sans/(.*)",
      "destination": "/object-sans/$1"
    }
  ]
}
